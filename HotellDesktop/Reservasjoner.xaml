<Window x:Class="HotellDesktop.Reservasjoner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Reservasjoner" Height="413.235" Width="588.235">
    <Grid>
        <ListView x:Name="GridReservasjoner" ItemsSource="{Binding}"  Margin="5,5,200,5">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="75" DisplayMemberBinding="{Binding Path=roomId}" Header="Room"/>
                    <GridViewColumn Width="75" DisplayMemberBinding="{Binding Path=firstName}" Header="Lastname" />
                    <GridViewColumn Width="75" DisplayMemberBinding="{Binding Path=lastName}" Header="Firstname" />
                    <GridViewColumn Width="75" DisplayMemberBinding="{Binding Path=checkInDate}" Header="Check in"/>
                    <GridViewColumn Width="75" DisplayMemberBinding="{Binding Path=checkOutDate}" Header="Check out"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
